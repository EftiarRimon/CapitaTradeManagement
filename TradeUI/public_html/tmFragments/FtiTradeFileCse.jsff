                                                       readOnly="false" disabled="false" autoSubmit="true"
                                                       immediate="false" valuePassThru="false" visible="true"
                                                       clientComponent="false"
                                                       binding="#{FtiTradeFilesCse.fileTypeRadio}">
                                        <af:selectItem label="Generate Text File" value="text" id="si3"/>
                                        <af:selectItem label="Generate XML File" value="xml" id="si4"/>
                                    </af:selectOneRadio>
                                    <af:separator id="s2"/>
                                </af:panelBox>
                            </af:panelGroupLayout>
                            <af:panelBox text="PanelBox3" id="pb4" showHeader="never" type="flow"
                                         inlineStyle="height:inherit; width:inherit;" partialTriggers="qryId1">
                                <f:facet name="toolbar"/>
                                <af:panelCollection id="pc1" inlineStyle="height:inherit; width:inherit;"
                                                    featuresOff="viewMenu">
                                    <f:facet name="menus">
                                        <af:menu text="View List" id="m1" inlineStyle="font-weight:bolder;"
                                                 icon="/images/list.png"/>
                                    </f:facet>
                                    <f:facet name="toolbar">
                                        <af:toolbar id="t2">
                                            <af:commandButton text="Select Template" id="cb3"
                                                              shortDesc="Choose Template &amp; Price File"
                                                              icon="#{resource['images:mailSend.jpg']}">
                                                <af:showPopupBehavior popupId="::sendCseMailPopup"
                                                                      triggerType="action"/>
                                            </af:commandButton>
                                        </af:toolbar>
                                    </f:facet>
                                    <f:facet name="statusbar">
                                        <af:toolbar id="t1">
                                            <af:outputText value="Total Record(s) Found : #{bindings.FtiTradeFilesCseVOIterator.estimatedRowCount}"
                                                           id="ot31" partialTriggers="::qryId1 ::pc1 resId1"/>
                                        </af:toolbar>
                                    </f:facet>
                                    <af:table value="#{bindings.FtiTradeFilesCseVO.collectionModel}" var="row"
                                              rows="#{bindings.FtiTradeFilesCseVO.rangeSize}"
                                              emptyText="#{bindings.FtiTradeFilesCseVO.viewable ? 'No data to display.' : 'Access Denied.'}"
                                              fetchSize="#{bindings.FtiTradeFilesCseVO.rangeSize}"
                                              rowBandingInterval="1"
                                              selectionListener="#{bindings.FtiTradeFilesCseVO.collectionModel.makeCurrent}"
