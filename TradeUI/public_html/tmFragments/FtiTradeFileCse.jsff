                                                   id="resId1c5" width="40">
                                            <af:outputText value="#{row.BuySell}" id="ot5"/>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.TradeQuantity.name}"
                                                   sortable="true"
                                                   headerText="#{bindings.FtiTradeFilesCseVO.hints.TradeQuantity.label}"
                                                   id="resId1c6" width="60">
                                            <af:outputText value="#{row.TradeQuantity}" id="ot6">
                                                <af:convertNumber groupingUsed="false"
                                                                  pattern="#{bindings.FtiTradeFilesCseVO.hints.TradeQuantity.format}"/>
                                            </af:outputText>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.Price.name}"
                                                   sortable="true"
                                                   headerText="#{bindings.FtiTradeFilesCseVO.hints.Price.label}"
                                                   id="resId1c7" width="60">
                                            <af:outputText value="#{row.Price}" id="ot7"/>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.Customer.name}"
                                                   sortable="true"
                                                   headerText="#{bindings.FtiTradeFilesCseVO.hints.Customer.label}"
                                                   id="resId1c1" width="60">
                                            <af:outputText value="#{row.Customer}" id="ot1"/>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.Opbrker.name}"
                                                   sortable="true"
                                                   headerText="#{bindings.FtiTradeFilesCseVO.hints.Opbrker.label}"
                                                   id="resId1c10" width="40">
                                            <af:outputText value="#{row.Opbrker}" id="ot10"/>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.Optrder.name}"
                                                   sortable="true"
                                                   headerText="#{bindings.FtiTradeFilesCseVO.hints.Optrder.label}"
                                                   id="resId1c12" width="40">
                                            <af:outputText value="#{row.Optrder}" id="ot12"/>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.TracerNo.name}"
                                                   sortable="true"
                                                   headerText="#{bindings.FtiTradeFilesCseVO.hints.TracerNo.label}"
                                                   id="resId1c4" width="60" visible="false">
                                            <af:outputText value="#{row.TracerNo}" id="ot4">
                                                <af:convertNumber groupingUsed="false"
                                                                  pattern="#{bindings.FtiTradeFilesCseVO.hints.TracerNo.format}"/>
                                            </af:outputText>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.TradeDate.name}"
                                                   sortable="true"
                                                   headerText="#{bindings.FtiTradeFilesCseVO.hints.TradeDate.label}"
                                                   id="resId1c8" width="60">
                                            <af:outputText value="#{row.TradeDate}" id="ot8">
                                                <af:convertDateTime pattern="#{bindings.FtiTradeFilesCseVO.hints.TradeDate.format}"/>
                                            </af:outputText>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.TradeTime.name}"
