<?xml version='1.0' encoding='UTF-8'?>
<ui:composition xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
                xmlns:f="http://java.sun.com/jsf/core">
    <af:pageTemplate viewId="/WEB-INF/templates/CapitaPageTemplate.jsf" id="pt1">
        <f:facet name="main">
            <af:group id="g2">
                <af:decorativeBox id="db1" inlineStyle="height:500px; width:860px;" dimensionsFrom="children">
                    <f:facet name="center">
                        <af:panelGroupLayout layout="vertical" id="pgl1">
                            <af:panelGroupLayout id="pgl2" halign="start" valign="top" layout="horizontal">
                                <af:panelBox text="PanelBox1" id="pb1" showHeader="never" inlineStyle="width:430px;"
                                             type="flow">
                                    <f:facet name="toolbar"/>
                                    <af:query id="qryId1" headerText="Search For CSE" disclosed="true"
                                              value="#{bindings.FtiTradeFilesCseVOCriteriaQuery.queryDescriptor}"
                                              model="#{bindings.FtiTradeFilesCseVOCriteriaQuery.queryModel}"
                                              queryListener="#{bindings.FtiTradeFilesCseVOCriteriaQuery.processQuery}"
                                              queryOperationListener="#{bindings.FtiTradeFilesCseVOCriteriaQuery.processQueryOperation}"
                                              resultComponentId="::pc1:resId1" saveQueryMode="hidden"
                                              modeChangeVisible="true" modeButtonPosition="footer"/>
                                </af:panelBox>
                                <af:panelBox text="Omnibus Trade File (CSE) " id="pb2" inlineStyle="width:400px;"
                                             type="flow">
                                    <f:facet name="toolbar">
                                        <af:group id="g1">
                                            <af:commandButton text="Validate" id="cb2" shortDesc="Validate File"
                                                              icon="/images/verify.png"
                                                              action="#{FtiTradeFilesCse.validateFileWithXsd}"
                                                              visible="#{FtiTradeFilesCse.fileTypeRadio.value eq 'xml'}"
                                                              partialTriggers="sor1"/>
                                            <af:commandButton text="Generate File" id="cb1" icon="/images/action.png"
