                                                              action="#{FtiTradeFilesCse.generateFile}"/>
                                        </af:group>
                                    </f:facet>
                                    <af:separator id="s1"/>
                                    <af:selectOneRadio id="sor1" layout="horizontal" value="text" showRequired="false"
