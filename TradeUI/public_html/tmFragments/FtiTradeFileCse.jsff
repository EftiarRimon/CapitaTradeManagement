                                              inlineStyle="height:inherit; width:inherit;" contentDelivery="immediate">
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.TraderId.name}"
                                                   sortable="true"
                                                   headerText="#{bindings.FtiTradeFilesCseVO.hints.TraderId.label}"
                                                   id="resId1c2" width="60">
                                            <af:outputText value="#{row.TraderId}" id="ot2"/>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.Scrip.name}"
                                                   sortable="true"
                                                   headerText="#{bindings.FtiTradeFilesCseVO.hints.Scrip.label}"
                                                   id="resId1c11" width="60">
                                            <af:outputText value="#{row.Scrip}" id="ot11"/>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.InstrumentCode.name}"
                                                   sortable="true"
                                                   headerText="#{bindings.FtiTradeFilesCseVO.hints.InstrumentCode.label}"
                                                   id="resId1c3" width="60">
                                            <af:outputText value="#{row.InstrumentCode}" id="ot3"/>
                                        </af:column>
                                        <af:column sortProperty="#{bindings.FtiTradeFilesCseVO.hints.BuySell.name}"
                                                   sortable="true"
