<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="purchase-power-update-flow">
    <template-reference>
      <document>/WEB-INF/templates/capita-task-flow-template.xml</document>
      <id>capita-task-flow-template</id>
    </template-reference>
    <default-activity>PurchasePowerCalculation</default-activity>
    <input-parameter-definition id="__2">
      <name>editable</name>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <managed-bean id="__1">
      <managed-bean-name>purchasePowerBean</managed-bean-name>
      <managed-bean-class>leads.capita.trade.view.backing.PurchasePowerBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="PurchasePowerCalculation">
      <page>/tmFragments/PurchasePowerCalculation.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
