<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="share-limit-flow">
    <template-reference>
      <document>/WEB-INF/templates/capita-task-flow-template.xml</document>
      <id>capita-task-flow-template</id>
    </template-reference>
    <default-activity>ShareLimit</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>ShareLimit</managed-bean-name>
      <managed-bean-class>leads.capita.trade.view.backing.ShareLimit</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>CSLimitBean</managed-bean-name>
      <managed-bean-class>leads.capita.trade.view.backing.CashLimitShareLimit</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="ShareLimit">
      <page>/tmFragments/ShareLimit.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
