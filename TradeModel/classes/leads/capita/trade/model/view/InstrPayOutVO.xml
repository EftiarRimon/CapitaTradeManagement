  </Variable>
  <Variable
    Name="processBy"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.current.sessionScope.get('CURRENT_USER') == null ? 'admin' : adf.context.current.sessionScope.get('CURRENT_USER')]]></TransientExpression>
  </Variable>
  <SQLQuery><![CDATA[SELECT rownum,
       InvestorAccounts.INVESTOR_CODE,
       InvestorAccounts.BOID,
       INVESTORACCOUNTS.BRANCH_ID,
       INVESTORACCOUNTS.BUSINESS_TYPE,
       InvestorAccounts.NAME,
       INVESTORACCOUNTS.Pay_Io_Policy,
       Products.PRODUCT_NAME,
       Products.Product_Type,
       Products.Trade_Code,
       Instruments.INSTRUMENT_NAME,
       Instruments.SHORT_NAME,
       INSTRUMENTS.ISIN,
       Exchanges.EXCHANGE_CODE,
       pkg_capita_utl.get_broker_code(InstrumentPayinOut.BROKER_ID,
                                      Exchanges.EXCHANGE_ID,
                                      Products.Product_Type) AS BROKER_CODE,
       InstrumentPayinOut.INS_SETTLE_ID,
       sum(InstrumentPayinOut.SELL) as SELL,
       sum(nvl(InstrumentPayinOut.BUY, 0)) as BUY,
       InstrumentPayinOut.Pay_In_Out_Date,
       InstrumentPayinOut.Pay_In_Out_Type,
       pkg_capita_utl.get_clearing_bo_for_broker(InstrumentPayinOut.BROKER_ID,
                                                 Exchanges.EXCHANGE_ID,
                                                 Products.Product_Type) AS Clearing_Bo_Id,
       Instruments.INSTRUMENT_ID
  FROM INVESTOR_ACCOUNTS    InvestorAccounts,
       PRODUCTS             Products,
       INSTRUMENTS          Instruments,
       EXCHANGES            Exchanges,
       BROKERS              Brokers,
       INSTRUMENT_PAYIN_OUT InstrumentPayinOut
 WHERE InvestorAccounts.ACCOUNT_ID = InstrumentPayinOut.ACCOUNT_ID
   AND InvestorAccounts.PRODUCT_ID = Products.PRODUCT_ID
   AND (InvestorAccounts.Pay_Io_Policy is null
   OR InvestorAccounts.Pay_Io_Policy not in ('NPO', 'NPIO'))
   AND InstrumentPayinOut.INSTRUMENT_ID = Instruments.INSTRUMENT_ID
   AND InstrumentPayinOut.EXCHANGE_ID = Exchanges.EXCHANGE_ID
   AND InstrumentPayinOut.BROKER_ID = Brokers.BROKER_ID
   AND InvestorAccounts.BUSINESS_TYPE = :b_Type
   AND trunc(InstrumentPayinOut.SETTLEMENT_DATE) >=
       trunc(pkg_capita_sys.get_current_system_date)
   AND (InstrumentPayinOut.BUY IS NOT NULL and InstrumentPayinOut.BUY > 0)
   AND InstrumentPayinOut.Process_By = :processBy

 group by InvestorAccounts.INVESTOR_CODE,
          InvestorAccounts.BOID,
          INVESTORACCOUNTS.BRANCH_ID,
          INVESTORACCOUNTS.BUSINESS_TYPE,
          InvestorAccounts.NAME,
          InvestorAccounts.Pay_Io_Policy,
          Products.PRODUCT_NAME,
          Products.Product_Type,
          Instruments.INSTRUMENT_NAME,
          INSTRUMENTS.ISIN,
          Exchanges.EXCHANGE_CODE,
          pkg_capita_utl.get_broker_code(InstrumentPayinOut.BROKER_ID,
                                         Exchanges.EXCHANGE_ID,
                                         Products.Product_Type),
          rownum,
          InstrumentPayinOut.INS_SETTLE_ID,
          InstrumentPayinOut.Pay_In_Out_Date,
          InstrumentPayinOut.Pay_In_Out_Type,
          Instruments.SHORT_NAME,
          pkg_capita_utl.get_clearing_bo_for_broker(InstrumentPayinOut.BROKER_ID,
                                                    Exchanges.EXCHANGE_ID,
                                                    Products.Product_Type),
          InstrumentPayinOut.SELL,
          Instruments.INSTRUMENT_ID,
          Products.Trade_Code,
          BROKERS.Clearing_Bo_Id
 ORDER BY rownum ASC]]></SQLQuery>
