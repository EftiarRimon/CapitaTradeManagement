<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="YearEndRevoke"
  Version="11.1.2.60.81"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="leads.capita.trade.model.view.YearEndRevoke_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[Select distinct t.trans_dt    Tran_Date,
                t.branch_code Branch_id,
                t.batch_no    Batch_no
  from gl_trans_hist t
 where t.narration = 'Closing Reversal'
   and t.trans_dt = (select max(m.trans_dt)
                       from gl_trans_hist m
                      where m.narration = 'Closing Reversal')
 group by t.branch_code, t.trans_dt, t.batch_no
 Order by t.trans_dt desc]]></SQLQuery>
  <ViewAttribute
    Name="TranDate"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="TRAN_DATE"
    Expression="TRAN_DATE"
    SQLType="DATE">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="leads.capita.trade.model.view.YearEndRevoke.TranDate_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="BranchId"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="BRANCH_ID"
    Expression="BRANCH_ID"
    SQLType="INTEGER">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="5"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="BranchId_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="BatchNo"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="BATCH_NO"
    Expression="BATCH_NO"
    SQLType="INTEGER">
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="BatchNo_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="TranDate"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="leads.capita.trade.TradeModelBundle"/>
  </ResourceBundle>
</ViewObject>
