  <SQLQuery><![CDATA[SELECT ROWNUM,
       LOWER('BLOCK_KEYS,') BLOCK_KEYS,
       '1,' BLOCKVER,
       'BRACEPSL,' EXECBROKER,
       ia.investor_code INVESTORCODE,
       (CASE WHEN ia.bic_code = '' THEN ',' ELSE ia.bic_code||',' END) INSTRPARTY,                
       LOWER(ia.investor_code) || to_char(t.trading_date, 'YYYYMMDD') || ROWNUM MASTERREF,
       
       LOWER('TRADE_SECURITY_ID,') TRADE_SECURITY_ID,
       ',ISIN,' AGENCYCODE,
       i.isin || ',' SECURITYCODE,
       TRIM(i.short_name) || ' ,EQT' SECURITYTYPE,
       LOWER('TRADE_INFO,') TRADE_INFO,
       t.buy_sell BUY_OR_SELL,
       ',BDT,' INDICATORNCURR,       
       TRIM(TO_CHAR(SUM(t.quantity * t.price) / SUM(t.quantity),'9999999999.9999')) RATE,
       SUM(t.quantity) QUANTITY,
       ROUND(SUM(t.quantity * t.price),2) TRADEAMOUNT,
       get_settlement_date(t.account_id,t.instrument_id,t.instrument_group,t.market_type,t.trading_date) SETTLEMENTDATE,
       t.trading_date TRADEDATE,       
       LOWER('BLOCK_TOTALS,') BLOCK_TOTALS,
       ',' TOTALSETTLAMT,
       --' ' SETTLCURRCODE,       
       (CASE t.buy_sell
         WHEN 'B' then
          ROUND((((SUM(t.quantity * t.price) / SUM(t.quantity)) * t.quantity) + t.commission),2)
         ELSE
          ROUND((((SUM(t.quantity * t.price) / SUM(t.quantity)) * t.quantity) - t.commission),2)
       END) TOTALNETCASH,
       ',,,Y' AUTOCREATECONFRM,       
       LOWER('BLOCK_COMMISSIONS,') BLOCK_COMMISSIONS,
       'FLAT,' COMMINDICATOR,
       'EXEC,' COMMTYPE,
       round((nvl(sum(t.commission), 0) -
             sum((t.quantity * t.price) *
                  pkg_charge.get_investor_charge_rate(t.account_id, 69) / 100)),
             2) COMMAMOUNT,       
       LOWER('BLOCK_CHARGES_TEXES,') BLOCK_CHARGES_TAXES,
       'OTHR,' CHAGTAXTYPE,
       sum((t.quantity * t.price) *
           pkg_charge.get_investor_charge_rate(t.account_id, 69) / 100) CHRGAMT,
       LOWER('BLOCK_SSI_KEYS,') BLOCK_SSI_KEYS,
       'ALRT,' INSTRINDICATOR,
       'BGD,' ALRTCOUNTRYCODE,
       'DSE,EQU,EQ-BRACEPSL' ALRTMETHODTYPE
  FROM trades t
 INNER JOIN investor_accounts ia
    ON ia.account_id = t.account_id
 INNER JOIN instruments i
    ON i.instrument_id = t.instrument_id
 WHERE ia.is_foreign = 'Y' AND ia.foreign_type = 'omgeo'
 GROUP BY rownum,
          ia.investor_code,
          ia.bic_code,
          i.short_name,
          t.trading_date,
          i.isin,
          t.buy_sell,
          t.price,
          t.quantity,
          t.commission,     
          t.account_id,
          t.instrument_id,
          t.instrument_group,
          t.market_type]]></SQLQuery>
