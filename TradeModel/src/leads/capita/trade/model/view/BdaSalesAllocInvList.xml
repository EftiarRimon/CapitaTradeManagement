  <SQLQuery><![CDATA[SELECT 
    InvestorAccounts.NAME, 
    InvestorAccounts.ACCOUNT_ID, 
    InvestorAccounts.BUSINESS_TYPE, 
    Products.PRODUCT_TYPE, 
    Products.PRODUCT_NAME, 
    Products.TRADE_CODE, 
    Products.PRODUCT_ID, 
    InstrumentBalances.SALABLE_QTY, 
    InstrumentBalances.INSTRUMENT_ID, 
    InstrumentBalances.TOTAL_QTY, 
    InstrumentBalances.AVG_COST, 
    instruments.INSTRUMENT_NAME, 
    InvestorAccounts.INVESTOR_CODE, 
    BdaOrders.ORDER_DATE, 
    BdaOrders.ORDER_ID,
    BdaOrders.TRADE_PRICE, 
    BdaOrders.TRADE_QUANTITY, 
    BdaOrderDetail.ORDER_QUANTITY, 
    BdaOrderDetail.DETAIL_ID,
    pkg_portfolio.get_purchase_power(InvestorAccounts.ACCOUNT_ID) as  purchase_power,
    pkg_portfolio.get_ledger_balance(InvestorAccounts.ACCOUNT_ID) as current_balance   
FROM 
    INSTRUMENT_BALANCES InstrumentBalances, 
    INVESTOR_ACCOUNTS InvestorAccounts, 
    PRODUCTS Products, 
    INSTRUMENTS instruments, 
    BDA_ORDERS BdaOrders, 
    BDA_ORDER_DETAIL BdaOrderDetail 
WHERE 
    InstrumentBalances.ACCOUNT_ID = InvestorAccounts.ACCOUNT_ID AND ((((InvestorAccounts.PRODUCT_ID = Products.PRODUCT_ID AND instruments.INSTRUMENT_ID = InstrumentBalances.INSTRUMENT_ID) AND BdaOrders.INSTRUMENT_ID = instruments.INSTRUMENT_ID) AND BdaOrders.ORDER_ID = BdaOrderDetail.ORDER_ID) AND BdaOrderDetail.ACCOUNT_ID = InvestorAccounts.ACCOUNT_ID) AND Products.PRODUCT_TYPE = 'BDA' AND InvestorAccounts.ACCOUNT_STATUS = 'ACTIVE'
AND BdaOrders.Buy_Sell='S'
AND nvl(BdaOrderDetail.ALLOC_QUANTITY,0)<1]]></SQLQuery>
