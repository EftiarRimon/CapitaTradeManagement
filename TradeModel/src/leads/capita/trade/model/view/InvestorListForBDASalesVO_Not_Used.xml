  <SQLQuery><![CDATA[SELECT InvestorAccounts.ACCOUNT_ID,  
       InvestorAccounts.INVESTOR_CODE, 
       InvestorAccounts.NAME,
       InvestorAccounts.PRODUCT_ID,
       Instruments.INSTRUMENT_NAME,
       Products.PRODUCT_NAME,
       InvestorFinancials.SALES_RECEIVABLE, 
       InvestorFinancials.PURCHASE_PAYABLE, 
       InvestorFinancials.AVAILABLE_BALANCE,
       InvestorFinancials.CURRENT_BALANCE,
       InvestorFinancials.PURCHASE_POWER,
       InstrumentBalances.AVG_COST,
       InstrumentBalances.TOTAL_QTY,
       nvl(InstrumentBalances.SALABLE_QTY,0)-nvl((SELECT SUM(ORDER_QTY) FROM BDA_SALES_ORDER bdas WHERE bdas.ACCOUNT_ID=InvestorAccounts.ACCOUNT_ID AND            bdas.INSTRUMENT_ID=Instruments.INSTRUMENT_ID AND bdas.STATUS='ORDER' AND                                                                                       to_char(bdas.ORDER_DATE,'yyyymmdd')=to_char(pkg_capita_sys.get_current_system_date ,'yyyymmdd')),0)               AS Salable,
       InstrumentBalances.SALABLE_QTY,
       InvestorAccounts.BANK_ACCOUNT_NO, 
       Products.PRODUCT_TYPE,
       Products.TRADE_CODE,
       Instruments.INSTRUMENT_TYPE,
       Instruments.INSTRUMENT_ID
FROM INSTRUMENT_BALANCES InstrumentBalances
, INVESTOR_FINANCIALS InvestorFinancials
, INVESTOR_ACCOUNTS InvestorAccounts
, PRODUCTS Products
, INSTRUMENTS Instruments
WHERE InstrumentBalances.ACCOUNT_ID = InvestorAccounts.ACCOUNT_ID
AND  InstrumentBalances.ACCOUNT_ID = InvestorFinancials.ACCOUNT_ID
AND  InvestorAccounts.PRODUCT_ID = Products.PRODUCT_ID
AND  InstrumentBalances.INSTRUMENT_ID = Instruments.INSTRUMENT_ID
AND  PRODUCTS.PRODUCT_TYPE='BDA'
AND InvestorAccounts.ACCOUNT_STATUS='ACTIVE'
AND  nvl(InstrumentBalances.SALABLE_QTY,0)>0
AND InvestorAccounts.BUSINESS_TYPE = :b_Type]]></SQLQuery>
