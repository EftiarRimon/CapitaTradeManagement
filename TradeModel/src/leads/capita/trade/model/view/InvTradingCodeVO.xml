<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="InvTradingCodeVO"
  Version="11.1.2.60.81"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false"
  FetchMode="FETCH_ALL"
  FetchSize="100"
  RangeSize="-1">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="leads.capita.trade.model.view.InvTradingCodeVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT 
    INVESTOR_ACCOUNTS.INVESTOR_CODE INVESTOR_CODE, 
    BROKER_MAPPING.TRADING_CODE TRADING_CODE, 
    PRODUCTS.PRODUCT_TYPE PRODUCT_TYPE 
FROM 
    INVESTOR_ACCOUNTS, 
    BROKER_MAPPING, 
    PRODUCTS 
WHERE 
    INVESTOR_ACCOUNTS.ACCOUNT_ID = BROKER_MAPPING.ACCOUNT_ID 
AND INVESTOR_ACCOUNTS.PRODUCT_ID = PRODUCTS.PRODUCT_ID 
AND (PRODUCTS.PRODUCT_TYPE = 'IDA' OR PRODUCTS.PRODUCT_TYPE = 'OWN')]]></SQLQuery>
  <ViewAttribute
    Name="InvestorCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="INVESTOR_CODE"
    Expression="INVESTOR_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="20"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="TradingCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="15"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TRADING_CODE"
    Expression="TRADING_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="15"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ProductType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PRODUCT_TYPE"
    Expression="PRODUCT_TYPE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="InvestorCode"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="leads.capita.trade.TradeModelBundle"/>
  </ResourceBundle>
</ViewObject>
