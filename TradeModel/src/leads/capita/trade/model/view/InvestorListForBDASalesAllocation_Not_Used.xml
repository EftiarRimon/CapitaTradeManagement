  <SQLQuery><![CDATA[SELECT BdaSalesOrder.ACCOUNT_ID, 
       BdaSalesOrder.SALES_ORDER_ID, 
       BdaSalesOrder.ORDER_QTY, 
       BdaSalesOrder.ORDER_DATE, 
       BdaSalesOrder.INSTRUMENT_ID, 
       BdaSalesOrder.STATUS, 
       investoraccounts.INVESTOR_CODE,
       investoraccounts.product_id,
       investoraccounts.NAME,
       products.product_name,
       products.trade_code,
       pkg_trade_file.get_bda_sell_quantity(products.trade_code, pkg_capita_sys.get_current_system_date, BdaSalesOrder.INSTRUMENT_ID) AS trade_qty,
       pkg_trade_file.get_bda_sell_price(products.trade_code, pkg_capita_sys.get_current_system_date, BdaSalesOrder.INSTRUMENT_ID) as TradePrice
FROM BDA_SALES_ORDER BdaSalesOrder
, INVESTOR_ACCOUNTS investoraccounts
,products products
WHERE BdaSalesOrder.ACCOUNT_ID = investoraccounts.ACCOUNT_ID
AND   investoraccounts.product_id=products.product_id
AND   BdaSalesOrder.Status='ORDER'
AND trunc(BdaSalesOrder.ORDER_DATE) =trunc(pkg_capita_sys.get_current_system_date)
AND InvestorAccounts.BUSINESS_TYPE = :b_Type]]></SQLQuery>
