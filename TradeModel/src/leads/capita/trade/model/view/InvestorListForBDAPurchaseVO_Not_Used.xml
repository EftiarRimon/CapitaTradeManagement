  <SQLQuery><![CDATA[SELECT InvestorAccounts.ACCOUNT_ID,  
       InvestorAccounts.INVESTOR_CODE, 
       InvestorAccounts.NAME,
       InvestorAccounts.PRODUCT_ID,
       InvestorAccounts.Sub_Product_Id,
       Products.PRODUCT_NAME,
       Products.trade_code,
       (select t.purchase_power from investor_financials t where t.account_id =InvestorAccounts.ACCOUNT_ID)  AS PurchasePower,
       (select sum(pt.price)  from bda_purchase_order pt where pt.account_id=InvestorAccounts.ACCOUNT_ID 
           and to_char(pt.order_date,'yyyymmdd')=to_char( pkg_capita_sys.get_current_system_date ,'yyyymmdd'))  AS OrderedAmnt,

       Products.PRODUCT_TYPE
FROM INVESTOR_ACCOUNTS InvestorAccounts
, PRODUCTS Products
WHERE InvestorAccounts.PRODUCT_ID = Products.PRODUCT_ID
AND  PRODUCTS.PRODUCT_TYPE='BDA'
AND InvestorAccounts.ACCOUNT_STATUS='ACTIVE'
--AND nvl((select t.purchase_power from investor_financials t where t.account_id =InvestorAccounts.ACCOUNT_ID),0)>0
AND InvestorAccounts.BUSINESS_TYPE = :b_Type]]></SQLQuery>
