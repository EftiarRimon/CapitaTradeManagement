  <SQLQuery><![CDATA[SELECT nvl(BdaOrders.TRADE_PRICE,0) TRADE_PRICE, 
       BdaOrders.EXCHANGE_ID, 
       BdaOrders.INSTRUMENT_ID, 
       BdaOrders.BUY_SELL, 
       BdaOrders.ALLOC_DATE, 
       BdaOrders.TRADE_CODE, 
       BdaOrders.ORDER_DATE, 
       BdaOrders.STATUS, 
       BdaOrderDetail.ALLOC_QUANTITY,
       BdaOrderDetail.ORDER_QUANTITY, 
       BdaOrderDetail.DETAIL_ID, 
       InvestorAccounts.ACCOUNT_STATUS, 
       InvestorAccounts.NAME, 
       InvestorAccounts.INVESTOR_CODE, 
       BdaOrders.ORDER_ID, 
       InvestorFinancials.Current_Balance, pkg_trade.get_bda_purchase_capacity(BdaOrders.ORDER_DATE,InvestorAccounts.ACCOUNT_ID) Purchase_Power,
       InvestorAccounts.ACCOUNT_ID
FROM BDA_ORDERS BdaOrders
, BDA_ORDER_DETAIL BdaOrderDetail
, INVESTOR_ACCOUNTS InvestorAccounts
, INVESTOR_FINANCIALS InvestorFinancials
,PRODUCTS Products
WHERE BdaOrders.ORDER_ID = BdaOrderDetail.ORDER_ID 
AND BdaOrderDetail.ACCOUNT_ID = InvestorAccounts.ACCOUNT_ID
AND InvestorFinancials.Account_Id=InvestorAccounts.Account_Id
AND Products.Product_Id=InvestorAccounts.Product_Id
AND Products.PRODUCT_TYPE = 'BDA' 
AND InvestorAccounts.ACCOUNT_STATUS = 'ACTIVE'
AND BdaOrders.Buy_Sell='B'
AND nvl(BdaOrderDetail.ALLOC_QUANTITY,0)<1]]></SQLQuery>
