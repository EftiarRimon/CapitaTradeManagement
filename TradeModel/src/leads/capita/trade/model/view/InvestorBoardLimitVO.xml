<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="InvestorBoardLimitVO"
  Version="11.1.2.60.81"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="leads.capita.trade.model.view.InvestorBoardLimitVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery><![CDATA[SELECT InvBoardLimitConfig.ACCOUNT_ID,
       pkg_investor.get_investor_code(InvBoardLimitConfig.ACCOUNT_ID) inv_code,
       InvBoardLimitConfig.BOARD_REF_CODE,
       InvBoardLimitConfig.LIMIT_PERCENTAGE,
       InvBoardLimitConfig.STATUS,
       InvBoardLimitConfig.Mbank_Limit,
       ia.mbank_id
  FROM INV_BOARD_LIMIT_CONFIG InvBoardLimitConfig, INVESTOR_ACCOUNTS ia
 WHERE InvBoardLimitConfig.Account_Id = ia.account_id
 AND InvBoardLimitConfig.Status = 'ACTIVE'
  ORDER BY InvBoardLimitConfig.Account_Id]]></SQLQuery>
  <ViewAttribute
    Name="AccountId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Long"
    ColumnType="LONGINT"
    AliasName="ACCOUNT_ID"
    Expression="ACCOUNT_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="InvCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="4000"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="INV_CODE"
    Expression="INV_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="4000"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="BoardRefCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="BOARD_REF_CODE"
    Expression="BOARD_REF_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="10"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="LimitPercentage"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="3"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="LIMIT_PERCENTAGE"
    Expression="LIMIT_PERCENTAGE"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="Status"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="STATUS"
    Expression="STATUS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="10"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="MbankLimit"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="18"
    Scale="4"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="MBANK_LIMIT"
    Expression="MBANK_LIMIT"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="MbankId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="3"
    Scale="0"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="MBANK_ID"
    Expression="MBANK_ID"
    SQLType="INTEGER"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="AccountId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="leads.capita.trade.TradeModelBundle"/>
  </ResourceBundle>
</ViewObject>
