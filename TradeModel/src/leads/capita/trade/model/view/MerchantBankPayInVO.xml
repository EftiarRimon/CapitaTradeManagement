    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.current.sessionScope.get('BUSINESS_TYPE') == null ? 'MBANK' : adf.context.current.sessionScope.get('BUSINESS_TYPE')]]></TransientExpression>
  </Variable>
  <SQLQuery><![CDATA[SELECT  rownum,InvestorAccounts.INVESTOR_CODE,
       InvestorAccounts.BOID,
       InvestorAccounts.ACCOUNT_ID,
       pkg_capita_utl.get_bo_for_investor(InvestorAccounts.ACCOUNT_ID)  as inv_bo,
       InvestorAccounts.ProductBO_ID,
       INVESTORACCOUNTS.BRANCH_ID,
       INVESTORACCOUNTS.BUSINESS_TYPE,
       InvestorAccounts.NAME,
       Products.PRODUCT_NAME,
       Products.Product_Type,
       Products.Trade_Code,  
       Instruments.INSTRUMENT_NAME,
       Instruments.SHORT_NAME,
       INSTRUMENTS.ISIN,
       Exchanges.EXCHANGE_CODE,
       pkg_capita_utl.get_broker_code (TradeInstrumentSettlements.BROKER_ID,Exchanges.EXCHANGE_ID) AS BROKER_CODE,
       pkg_capita_utl.get_clearing_bo_for_broker (TradeInstrumentSettlements.BROKER_ID,Exchanges.EXCHANGE_ID) AS Clearing_Bo_Id,
       TradeInstrumentSettlements.INS_SETTLE_ID,
       sum(TradeInstrumentSettlements.SELL) as SELL,
       sum(nvl(TradeInstrumentSettlements.BUY,0)) as BUY,
       TradeInstrumentSettlements.SETTLEMENT_DATE,
       TradeInstrumentSettlements.TRADING_DATE,
       Instruments.INSTRUMENT_ID
FROM INVESTOR_ACCOUNTS InvestorAccounts
,PRODUCTS Products
,INSTRUMENTS Instruments
,EXCHANGES Exchanges
,BROKERS Brokers
,TRADE_INSTRUMENT_SETTLEMENTS TradeInstrumentSettlements
WHERE InvestorAccounts.ACCOUNT_ID = TradeInstrumentSettlements.ACCOUNT_ID
AND InvestorAccounts.PRODUCT_ID = Products.PRODUCT_ID
AND  TradeInstrumentSettlements.INSTRUMENT_ID= Instruments.INSTRUMENT_ID
AND  TradeInstrumentSettlements.EXCHANGE_ID= Exchanges.EXCHANGE_ID
AND  TradeInstrumentSettlements.BROKER_ID= Brokers.BROKER_ID
--AND Products.PRODUCT_TYPE !='OWN' --For IL
AND InvestorAccounts.BUSINESS_TYPE = :b_Type
AND  (TradeInstrumentSettlements.SELL IS NOT NULL and TradeInstrumentSettlements.SELL>0)
group by InvestorAccounts.ProductBO_ID,Instruments.SHORT_NAME,InvestorAccounts.BOID,INVESTORACCOUNTS.BRANCH_ID
,INVESTORACCOUNTS.BUSINESS_TYPE,InvestorAccounts.NAME,Products.PRODUCT_NAME,Instruments.INSTRUMENT_NAME,INSTRUMENTS.ISIN
,Exchanges.EXCHANGE_CODE,pkg_capita_utl.get_broker_code (TradeInstrumentSettlements.BROKER_ID,Exchanges.EXCHANGE_ID), TradeInstrumentSettlements.INS_SETTLE_ID, TradeInstrumentSettlements.SELL,TradeInstrumentSettlements.SETTLEMENT_DATE
,Instruments.INSTRUMENT_ID,Products.Trade_Code
,InvestorAccounts.INVESTOR_CODE,rownum
 ,pkg_capita_utl.get_bo_for_investor(InvestorAccounts.ACCOUNT_ID)
,pkg_capita_utl.get_clearing_bo_for_broker (TradeInstrumentSettlements.BROKER_ID,Exchanges.EXCHANGE_ID)
,InvestorAccounts.ACCOUNT_ID
,TradeInstrumentSettlements.TRADING_DATE
,Products.Product_Type]]></SQLQuery>
