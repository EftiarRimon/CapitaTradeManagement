<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="FlexTradeLimitLOV"
  Version="11.1.2.60.81"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="leads.capita.trade.model.view.lov.FlexTradeLimitLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="sys_date"
    Kind="where"
    Type="java.sql.Timestamp">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
    <TransientExpression><![CDATA[adf.context.current.sessionScope.get('SYSTEM_DATE_TIMESTAMP')]]></TransientExpression>
  </Variable>
  <SQLQuery><![CDATA[select flex_trade_limit.limit_id,
        flex_trade_limit.version_no,
        'Version-' || flex_trade_limit.version_no ||' ('
        || to_char( to_timestamp(flex_trade_limit.gen_time, 'yyyy-MM-dd HH:MI:SS.FF AM'),'HH:MI:SS AM')||')' version_name,
       flex_trade_limit.app_date,
       flex_trade_limit.gen_time,
       flex_trade_limit.morn_intraday,
       flex_trade_limit.is_deactivate_all,
       flex_trade_limit.file_type
       from flex_trade_limit 
       where flex_trade_limit.app_date = :sys_date]]></SQLQuery>
  <ViewAttribute
    Name="LimitId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="LIMIT_ID"
    Expression="LIMIT_ID"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="VersionNo"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="VERSION_NO"
    Expression="VERSION_NO"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="VersionName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="48"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VERSION_NAME"
    Expression="VERSION_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="48"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="AppDate"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="APP_DATE"
    Expression="APP_DATE"
    SQLType="DATE"/>
  <ViewAttribute
    Name="GenTime"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="GEN_TIME"
    Expression="GEN_TIME"
    SQLType="VARCHAR"
    Precision="255"/>
  <ViewAttribute
    Name="MornIntraday"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MORN_INTRADAY"
    Expression="MORN_INTRADAY"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="IsDeactivateAll"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="IS_DEACTIVATE_ALL"
    Expression="IS_DEACTIVATE_ALL"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="FileType"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FILE_TYPE"
    Expression="FILE_TYPE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="VersionNo"/>
    <Item Value="AppDate"/>
  </AttrArray>
</ViewObject>
