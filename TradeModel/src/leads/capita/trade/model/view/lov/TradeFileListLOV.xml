<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="TradeFileListLOV"
  Version="11.1.2.60.81"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="leads.capita.trade.model.view.lov.TradeFileListLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="sys_date"
    Kind="where"
    Type="java.sql.Timestamp">
    <TransientExpression><![CDATA[adf.context.current.sessionScope.get('SYSTEM_DATE_TIMESTAMP')]]></TransientExpression>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT ImportExtFiles.IMPORT_ID, 
       ImportExtFiles.FILE_ID, 
       ImportExtFiles.FILE_NAME, 
       ImportExtFiles.FILE_DATE
  FROM IMPORT_EXT_FILES ImportExtFiles, TRADE_FILE_CONF TradeFileConf
  WHERE ImportExtFiles.File_Id = TradeFileConf.File_Id
  AND  ImportExtFiles.File_Date = :sys_date
  AND  TradeFileConf.File_Type = 'T'
  ORDER BY TradeFileConf.Exchange_Id, ImportExtFiles.Import_Id]]></SQLQuery>
  <ViewAttribute
    Name="ImportId"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="IMPORT_ID"
    Expression="IMPORT_ID"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="FileId"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FILE_ID"
    Expression="FILE_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="FileName"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="FILE_NAME"
    Expression="FILE_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="FileDate"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.sql.Timestamp"
    ColumnType="DATE"
    AliasName="FILE_DATE"
    Expression="FILE_DATE"
    SQLType="DATE"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="ImportId"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="leads.capita.trade.TradeModelBundle"/>
  </ResourceBundle>
</ViewObject>
