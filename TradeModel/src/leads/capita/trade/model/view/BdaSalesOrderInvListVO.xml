  <SQLQuery><![CDATA[SELECT 
    InvestorAccounts.NAME, 
    InvestorAccounts.ACCOUNT_ID, 
    InvestorAccounts.BUSINESS_TYPE, 
    Products.PRODUCT_TYPE, 
    Products.PRODUCT_NAME, 
    Products.TRADE_CODE, 
    Products.PRODUCT_ID, 
    InstrumentBalances.SALABLE_QTY, 
    InstrumentBalances.INSTRUMENT_ID, 
    InstrumentBalances.ACCOUNT_ID AS ACCOUNT_ID1, 
    InstrumentBalances.TOTAL_QTY, 
    InstrumentBalances.AVG_COST, 
    instruments.INSTRUMENT_NAME, 
    InvestorAccounts.INVESTOR_CODE,
    pkg_portfolio.get_purchase_power(InvestorAccounts.ACCOUNT_ID) as purchase_power,
    pkg_portfolio.get_ledger_balance(InvestorAccounts.ACCOUNT_ID) as current_balance  
FROM 
    INSTRUMENT_BALANCES InstrumentBalances, 
    INVESTOR_ACCOUNTS InvestorAccounts, 
    PRODUCTS Products, 
    INSTRUMENTS instruments 
WHERE 
    InstrumentBalances.ACCOUNT_ID = InvestorAccounts.ACCOUNT_ID 
AND InvestorAccounts.PRODUCT_ID = Products.PRODUCT_ID
AND instruments.INSTRUMENT_ID =InstrumentBalances.INSTRUMENT_ID
AND InvestorAccounts.ACCOUNT_STATUS='ACTIVE'
AND Products.PRODUCT_TYPE = 'BDA' AND InvestorAccounts.BUSINESS_TYPE = :bType
AND InvestorAccounts.ACCOUNT_ID NOT IN (select account_id from bda_order_detail where order_id=:order_id)
AND instruments.INSTRUMENT_ID =:INSTRUMENT_ID]]></SQLQuery>
