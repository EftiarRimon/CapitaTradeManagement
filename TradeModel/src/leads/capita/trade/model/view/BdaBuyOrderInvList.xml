  </ListBinding>
  <SQLQuery><![CDATA[SELECT 
    InvestorAccounts.NAME, 
    InvestorAccounts.ACCOUNT_ID, 
    InvestorAccounts.BUSINESS_TYPE, 
    Products.PRODUCT_TYPE, 
    Products.PRODUCT_NAME, 
    Products.TRADE_CODE, 
    Products.PRODUCT_ID, 
    PKG_TRADE_FILE.get_bda_buy_order_price(pkg_capita_sys.get_current_system_date,InvestorAccounts.ACCOUNT_ID) TotalOrderPrice,
    InvestorFinancials.Current_Balance,
    InvestorFinancials.Purchase_Power, 
    InvestorAccounts.INVESTOR_CODE,
 pkg_portfolio.get_avg_cost(InvestorAccounts.ACCOUNT_ID,:p_instrument_id) as avg_cost,
    pkg_portfolio.get_total_qty(InvestorAccounts.ACCOUNT_ID,:p_instrument_id) as total_share 
FROM 
    INVESTOR_FINANCIALS InvestorFinancials, 
    INVESTOR_ACCOUNTS InvestorAccounts, 
    PRODUCTS Products
WHERE 
    InvestorFinancials.ACCOUNT_ID = InvestorAccounts.ACCOUNT_ID 
AND InvestorAccounts.PRODUCT_ID = Products.PRODUCT_ID
AND InvestorAccounts.ACCOUNT_STATUS='ACTIVE'
AND Products.PRODUCT_TYPE = 'BDA' AND InvestorAccounts.BUSINESS_TYPE = :bType
AND InvestorAccounts.ACCOUNT_ID NOT IN (select account_id from bda_order_detail where order_id=:order_id)]]></SQLQuery>
