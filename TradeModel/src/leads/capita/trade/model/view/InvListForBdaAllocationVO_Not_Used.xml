  <SQLQuery><![CDATA[SELECT  InvestorAccounts.INVESTOR_CODE, 
        InvestorAccounts.NAME, 
        BdaPurchaseOrder.ORDER_QTY,
        BdaPurchaseOrder.ALLOCATION_QTY,
        BdaPurchaseOrder.ORDER_DATE, 
        BdaPurchaseOrder.PRICE,
        Products.PRODUCT_NAME,
        Products.TRADE_CODE,
        Instruments.INSTRUMENT_NAME,
        Instruments.SHORT_NAME,
        BdaPurchaseOrder.PURCHASE_ORDER_ID,
        InvestorAccounts.ACCOUNT_ID,  
        Instruments.INSTRUMENT_ID ,
        BdaPurchaseOrder.STATUS,
        pkg_trade_file.get_bda_buy_quantity(products.trade_code, pkg_capita_sys.get_current_system_date, Instruments.INSTRUMENT_ID) as trade_qty,
        pkg_trade_file.get_bda_buy_price(products.trade_code, pkg_capita_sys.get_current_system_date, Instruments.INSTRUMENT_ID)  as trade_price
FROM BDA_PURCHASE_ORDER BdaPurchaseOrder
, INVESTOR_ACCOUNTS InvestorAccounts
, PRODUCTS Products
, INSTRUMENTS Instruments
WHERE BdaPurchaseOrder.ACCOUNT_ID = InvestorAccounts.ACCOUNT_ID
AND  InvestorAccounts.PRODUCT_ID = Products.PRODUCT_ID 
AND  BdaPurchaseOrder.INSTRUMENT_ID = Instruments.INSTRUMENT_ID
AND  BdaPurchaseOrder.STATUS='ORDER'
AND trunc(BdaPurchaseOrder.ORDER_DATE) =trunc(pkg_capita_sys.get_current_system_date)
AND InvestorAccounts.BUSINESS_TYPE = :b_Type]]></SQLQuery>
