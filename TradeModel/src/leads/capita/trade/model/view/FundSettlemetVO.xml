  <SQLQuery><![CDATA[select tn.inv_netted_id id, tn.exchange_id, ex.short_name, tn.account_id, investor_code, buy_amount, sell_amount, 
netted_amount, tn.is_foreign, howla_type, trading_date, tn.settlement_date
  from trade_investor_netting tn, investor_accounts ac, exchanges ex
where ac.account_id=tn.account_id and  ex.exchange_id = tn.exchange_id
and (upper(howla_type)='D' or upper(tn.is_foreign)='T')
order by settlement_date]]></SQLQuery>
